	            {% if item["faqs_question_1"] != blank and item["faqs_answer_1"] != blank %}
		            <li class="faq-list__item" {{ block.shopify_attributes }}>
			            <a href="#" class="faq-list__item-question">
				            <h3>{{ item["faqs_question_1"] }}</h3>
				            {%- render 'icon-down-arrow' -%}
				            {%- render 'icon-up-arrow' -%}
                          </a>
                      <div class="faq-list__item-answer"><p>{{ item["faqs_answer_1"] }}</p></div>
		            </li>
	            {%- endif -%}
