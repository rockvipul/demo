<div id="shopify-section-custom_faq_section" class="faq-section">
   <div class="page-title">
      <div class="container">
         <div class="row">
            {% if metafields.custom_fields["faqs_subheading"] != blank %}
	            <p class="page-title__subheading subheading-text">
	               {{ metafields.custom_fields["faqs_subheading"] }}
	            </p>
            {%- endif -%}
            {% if metafields.custom_fields["faqs_heading"] != blank %}
            	<h2 class="page-title__heading">{{ metafields.custom_fields["faqs_heading"] }}</h2>
            {%- endif -%}
         </div>
      </div>
   </div>
   {% if metafields.custom_fields["product_faqs"] != blank %}
   <div class="faq-content">
      <div class="container">
