<div class="jb-product-collateral">
  <div class="tab-content">
    <div class="tabs" id="tab5" {% if block.settings.background != blank %} style="background-color:{{block.settings.background}};" {% endif %}>
<div class="container">
      <div class="tab-content-title">
      {% if block.settings.title != blank %}
        {{ block.setting }}
	  {% endif %}
      </div>
      <div class="tab-content-full-inner">
         <div class="image-section">
            <div class="grid">
               <div class="grid-image">
                 {% if block.settings.icon1_image != blank %}
                 	<img src="{{ block.settings.icon1_image | img_url: '1024x'}}">
                 {% endif %}
               </div>
               <div class="grid-heading">
               {% if block.settings.icon1_title != blank %}
               	{{ block.settings.icon1_title }}
               {% endif %}
               </div>
               <div class="grid-desc">
              	{% if block.settings.icon1_detail != blank %}
                	{{ block.settings.icon1_detail }}
                {% endif %}
               </div>
            </div>
            <div class="grid">
               <div class="grid-image">
                 {% if block.settings.icon2_image != blank %}
